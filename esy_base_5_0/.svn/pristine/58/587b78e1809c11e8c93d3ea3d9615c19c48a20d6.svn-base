{
	"templates":["entity","dao","dao.impl","service","service.impl","controller"],
	"name":"Person",
	"package":"org.yessoft.base",
	"entity":{
		"table":"base_mperson",
		"title":"人员信息",
		"defaultOrders":[
			{
				"field":"updated",
				"sort":"desc"
			}
		],
		"otherOrders":[
			{
				"name":"pid",
				"orders":[
					{
						"field":"pid",
						"sort":"asc"
					}
				]	
			}
		],
		"properties":[
			{
				"name":"eid",
				"type":"String",
				"format":"String",
				"length":255,
				"title":"路径",
				"field":"mp_eid",
				"nullable":false,
				"index":true,
				"default":"",
				"conditions":["eq","match"],
				"memo":"企业路径，格式：00000000.00000000.00000000"
			},{
				"name":"pid",
				"type":"String",
				"format":"String",
				"length":32,
				"title":"人员编号",
				"field":"mp_pid",
				"nullable":false,
				"index":true,
				"default":"",
				"conditions":["eq"],
				"memo":"人员编号"
			},{
				"name":"cname",
				"type":"String",
				"format":"String",
				"length":64,
				"title":"中文名",
				"field":"mp_cname",
				"nullable":false,
				"index":false,
				"default":"",
				"conditions":["match"],
				"memo":"中文姓名。"
			},{
				"name":"ename",
				"type":"String",
				"format":"String",
				"length":64,
				"title":"英文名",
				"field":"mp_ename",
				"nullable":true,
				"index":false,
				"default":"",
				"conditions":["match"],
				"memo":"英文姓名。"
			},{
				"name":"sex",
				"type":"String",
				"format":"String",
				"length":1,
				"title":"性别",
				"field":"mp_sex",
				"nullable":true,
				"index":false,
				"default":"",
				"conditions":["eq"],
				"memo":"性别"
			},{
				"name":"officePhone",
				"type":"String",
				"format":"String",
				"length":64,
				"title":"办公电话",
				"field":"mp_officePhone",
				"nullable":true,
				"index":false,
				"default":"",
				"conditions":["eq"],
				"memo":"办公电话。"
			},{
				"name":"mobilePhone",
				"type":"String",
				"format":"String",
				"length":64,
				"title":"移动电话",
				"field":"mp_mobilePhone",
				"nullable":true,
				"index":false,
				"default":"",
				"conditions":["eq"],
				"memo":"移动电话。"
			},{
				"name":"mail",
				"type":"String",
				"format":"String",
				"length":128,
				"title":"电子邮件",
				"field":"mp_mail",
				"nullable":true,
				"index":false,
				"default":"",
				"conditions":["match"],
				"memo":"电子邮件。"
			},{
				"name":"enable",
				"type":"Boolean",
				"length":0,
				"title":"状态",
				"field":"mp_enable",
				"nullable":false,
				"index":false,
				"default":true,
				"conditions":["eq"],
				"memo":"状态（true:有效/false:停用）"
			}
		]
	},
	"dao":{
		"methods":[
		]
	},
	"service":{
		"methods":[
		]
	},	
	"controller":{
		"requestMapping":"/api/base/person"
	}
}